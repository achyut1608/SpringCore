<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="questionPaper" class="Pojo.QuestionPaper">
		<constructor-arg value="Chemical"></constructor-arg>
		<constructor-arg>
			<map>
				<entry key = "Question1" value="Answer1"></entry>
				<entry key = "Question2" value="Answer2"></entry>
				<entry key = "Question3" value="Answer3"></entry>
				
			</map>
		</constructor-arg>
	</bean>

</beans>